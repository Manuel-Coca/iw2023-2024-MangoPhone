package es.uca.iw.aplication.service;

import org.springframework.stereotype.Service;

import es.uca.iw.aplication.repository.CuentaUsuarioRepository;
import es.uca.iw.aplication.tables.usuarios.CuentaUsuario;

@Service
public class ContratoService {
    private final ContratoRepository contratoRepository;

    public CuentaUsuarioService(ContratoRepository contratoRepository) { this.contratoRepository = contratoRepository; }

    public void createContrato(Contrato contrato){
        cuentaUsuarioRepository.save(contrato);
    }
}